{
  "editor.rulers": [
    80
  ],
  "githubIssues.createIssueTriggers": [
    "feature",
    "bug",
    "documentation",
    "question"
  ],
  "githubIssues.issueCompletions.enabled": true,
  "githubIssues.useBranchForIssues": "prompt",
  "githubIssues.issueBranchTitle": "$feature/${issueNumber}-${sanitizedIssueTitle}",
  "githubIssues.queries": [
    {
      "label": "My Open Issues",
      "query": "assignee:${user} state:open"
    },
    {
      "label": "Issues",
      "query": "repo:${owner}/${repository} state:open" //  sort:created-desc
    }
  ],
  "python.formatting.provider": "yapf",
  "python.autoComplete.extraPaths": [
    "${env:AMBF_WS}/",
    "${env:AMBF_WS}/ambf_ros_modules/",
    "${env:AMBF_WS}/ambf_ros_modules/ambf_comm/",
    "${env:AMBF_WS}/ambf_ros_modules/ambf_comm/scripts/",
    "${env:AMBF_WS}/ambf_controller/dvrk_functions/scripts/",
    "${env:AMBF_RL_WS}/**",
    "${env:AMBF_RL_WS}/envs/**",
    "${env:AMBF_RL_WS}/envs/ambf_rl/**",
    "${env:AMBF_RL_WS}/envs/dVRK/**",
    "${env:AMBF_RL_WS}/scripts/**",
  ],
  "python.analysis.extraPaths": [
    "${env:AMBF_WS}/**",
    "${env:AMBF_WS}/ambf_ros_modules/**",
    "${env:AMBF_WS}/ambf_ros_modules/ambf_comm/**",
    "${env:AMBF_WS}/ambf_ros_modules/ambf_comm/scripts/**",
    "${env:AMBF_WS}/ambf_controller/dvrk_functions/scripts/**",
    "${env:AMBF_RL_WS}/**",
    "${env:AMBF_RL_WS}/envs/**",
    "${env:AMBF_RL_WS}/envs/ambf_rl/**",
    "${env:AMBF_RL_WS}/envs/dVRK/**",
    "${env:AMBF_RL_WS}/scripts/**",
  ],
  "python.pythonPath": "pyenv_arl_training/bin/python"
}